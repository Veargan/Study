<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                  assembly="PersonForm"
                  namespace="PersonForm">
  <class name="Person">
    <id name="id" column="id">
      <generator class="identity" />
    </id>
    <property name="fname" />
    <property name="lname" />
    <property name="age" />
  </class>
</hibernate-mapping>